        <!-- End Page wrapper  -->
        <script>
        function initMap() {
            var uluru = {lat: -7.2793795, lng: 112.7926759};
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 14,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
        </script>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA-XjofybTWwQTy9HYa8zx-Hh9No6BaQSI
        &callback=initMap"></script>
